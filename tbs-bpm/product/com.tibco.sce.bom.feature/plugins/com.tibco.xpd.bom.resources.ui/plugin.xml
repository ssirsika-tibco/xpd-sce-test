<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
 <extension
       point="com.tibco.xpd.resources.specialFolders">
    <folder
          allowDuplicateResources="false"
          icon="icons/Bom_SpecialFolder.png"
          issueId="bom.duplicateResourceIssue"
          kind="bom"
          name="%specialFolder_name"
          navigatorContentId="com.tibco.xpd.projectExplorer.bomContent"
          projectAssetId="com.tibco.xpd.asset.bom">
    </folder>
 </extension>
 <extension
       point="org.eclipse.ui.navigator.navigatorContent">
    <navigatorContent
          activeByDefault="true"
          contentProvider="com.tibco.xpd.bom.resources.ui.internal.navigator.BOMContentProvider"
          icon="icons/Bom_SpecialFolder.png"
          id="com.tibco.xpd.projectExplorer.bomContent"
          labelProvider="com.tibco.xpd.bom.resources.ui.internal.navigator.BOMLabelProvider"
          name="%navigatorContent_name"
          priority="high"
          providesSaveables="true">
       <triggerPoints>
       		<or>
				<and>
					<instanceof value="org.eclipse.core.resources.IProject"/>
						<test
							property="org.eclipse.core.resources.projectNature"
	                        value="com.tibco.xpd.resources.xpdNature"/>
	                    <test
	                        property="org.eclipse.core.resources.open"
	                        value="true"/>
				</and>
				<and>
					<instanceof value="org.eclipse.core.resources.IFolder"/>
					<test
						property="com.tibco.xpd.resources.specialfolder.containsSpecialFolder"
						value="bom"/>
				</and>
				<and>
					<instanceof value="com.tibco.xpd.resources.projectconfig.SpecialFolder"/>
					<test
						property="com.tibco.xpd.resources.specialfolder.isOfKind"
						value="bom"/>
				</and>
			<and>
	               <instanceof
	                     value="org.eclipse.core.resources.IFile">
	               </instanceof>
	               <test
	                     property="org.eclipse.core.resources.extension"
	                     value="bom">
	               </test>
                <test
                      property="com.tibco.xpd.resources.specialfolder.isRootSpecialFolder"
                      value="bom">
                </test>
            	</and>
			</or>
       </triggerPoints>
       <possibleChildren>
            <or>
            	<and>
                	 <instanceof value="com.tibco.xpd.resources.projectconfig.SpecialFolder"/>
                 	<test
                       property="com.tibco.xpd.resources.specialfolder.isOfKind"
                       value="bom"/>
             	</and>
             	<and>
	               <instanceof
	                     value="org.eclipse.emf.ecore.EObject">
	               </instanceof>
	               <test
	                     property="com.tibco.xpd.resources.eobjecttest.isFromPackage"
	                     value="uml">
	               </test>
            	</and>
              <instanceof
                    value="org.eclipse.core.resources.IFile">
              </instanceof>
              <instanceof
                    value="org.eclipse.core.resources.IFolder">
              </instanceof>
              <instanceof
                    value="org.eclipse.gmf.runtime.notation.Diagram">
              </instanceof>
              <instanceof
                    value="com.tibco.xpd.bom.resources.ui.providers.DiagramGroupTransientItemProvider">
              </instanceof>
            </or>
       </possibleChildren>
       <override
             policy="InvokeAlwaysRegardlessOfSuppressedExt"
             suppressedExtensionId="org.eclipse.ui.navigator.resourceContent">
       </override>
       <actionProvider
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.actions.BOMAddChildActionProvider"
             id="com.tibco.xpd.projectExplorer.action.bom.addChildActionProvider">
          <enablement>
             <or>
                <instanceof
                      value="org.eclipse.uml2.uml.Package">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Class">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Enumeration">
                </instanceof>
             </or>
          </enablement>
       </actionProvider>
       <actionProvider
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.actions.BOMConvertActionProvider"
             id="com.tibco.xpd.projectExplorer.action.bom.classConvertActionProvider">
          <enablement>
             <or>
                <instanceof
                      value="org.eclipse.uml2.uml.Class">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Property">
                </instanceof>
             </or>
          </enablement>
       </actionProvider>
       <actionProvider
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.actions.BOMEditActionProvider"
             id="com.tibco.xpd.projectExplorer.action.bom.editActionProvider">
          <enablement>
           <or>
              <and>
                 <instanceof
                       value="org.eclipse.emf.ecore.EObject">
                 </instanceof>
                 <test
                       property="com.tibco.xpd.resources.eobjecttest.isFromPackage"
                       value="uml">
                 </test>
              </and>
              <instanceof
                    value="org.eclipse.gmf.runtime.notation.Diagram">
              </instanceof></or>
          </enablement>
       </actionProvider>
       <actionProvider
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.actions.UndoRedoActionProvider"
             id="com.tibco.xpd.projectExplorer.action.bom.undoRedoActionProvider"
             overrides="org.eclipse.ui.navigator.resources.UndoRedoActionProvider">
          <enablement>
           <or>
              <and>
                 <instanceof
                       value="org.eclipse.emf.ecore.EObject">
                 </instanceof>
                 <test
                       property="com.tibco.xpd.resources.eobjecttest.isFromPackage"
                       value="uml">
                 </test>
              </and>
              <instanceof
                    value="org.eclipse.gmf.runtime.notation.Diagram">
              </instanceof>
              <instanceof
                    value="com.tibco.xpd.bom.resources.ui.providers.DiagramGroupTransientItemProvider">
              </instanceof></or>
          </enablement>
       </actionProvider>
       <dropAssistant
             class="com.tibco.xpd.ui.projectexplorer.dropassist.SpecialFolderDropAssist"
             id="com.tibco.xpd.bom.resources.ui.specialFolder.dropAssistant">
          <possibleDropTargets>
             <or>
                <and>
                   <instanceof
                         value="com.tibco.xpd.resources.projectconfig.SpecialFolder">
                   </instanceof>
                   <test
                         property="com.tibco.xpd.resources.specialfolder.isOfKind"
                         value="bom">
                   </test>
                </and>
                <and>
                   <instanceof
                         value="org.eclipse.core.resources.IFolder">
                   </instanceof>
                   <test
                         property="com.tibco.xpd.resources.specialfolder.isRootSpecialFolder"
                         value="bom">
                   </test>
                </and>
             </or>
          </possibleDropTargets>
       </dropAssistant>
       <dropAssistant
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.BOMDropAdapterAssistant"
             id="com.tibco.xpd.bom.resources.ui.BOMDropAssistant">
          <possibleDropTargets>
             <or>
                <instanceof
                      value="org.eclipse.uml2.uml.Package">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Class">
                </instanceof>
             </or>
          </possibleDropTargets>
       </dropAssistant>
       <commonSorter
             class="com.tibco.xpd.bom.resources.ui.internal.navigator.sorter.BOMContentSorter"
             id="com.tibco.xpd.projectExplorer.bomContent.sorter">
          <parentExpression>
             <or>
                <and>
                   <instanceof
                         value="com.tibco.xpd.resources.projectconfig.SpecialFolder">
                   </instanceof>
                   <test
                         property="com.tibco.xpd.resources.specialfolder.isOfKind"
                         value="bom">
                   </test>
                </and>
                <instanceof
                      value="org.eclipse.core.resources.IFile">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Model">
                </instanceof>
                <instanceof
                      value="org.eclipse.uml2.uml.Class">
                </instanceof>
             </or>
          </parentExpression>
       </commonSorter>
    </navigatorContent>
    <commonWizard
          associatedExtensionId="com.tibco.xpd.projectExplorer.bomContent"
          type="export"
          wizardId="com.tibco.xpd.ui.importexport.exportwizard.documentation.DocExportWizard">
       <enablement>
          <or>
             <and>
                <instanceof
                      value="org.eclipse.core.resources.IFile">
                </instanceof>
                <test
                      property="org.eclipse.core.resources.extension"
                      value="bom">
                </test>
             </and>
          </or>
       </enablement>
    </commonWizard>
 </extension>

 <extension
       point="org.eclipse.core.runtime.adapters">
    <factory
          adaptableType="org.eclipse.uml2.uml.Element"
          class="org.eclipse.gmf.runtime.common.ui.services.properties.PropertiesServiceAdapterFactory">
       <adapter
             type="org.eclipse.ui.views.properties.IPropertySource">
       </adapter>
    </factory>
 </extension>
 <extension
       point="com.tibco.xpd.resources.specialFolderFileBinding">
    <binding
          kind="bom">
       <filePatterns
             match=".*\.[bB][oO][mM]">
       </filePatterns>
    </binding>
 </extension>
  <extension
       point="org.eclipse.emf.ecore.generated_package">
    <package
          class="com.tibco.xpd.bom.resources.ui.bomnotation.BomNotationPackage"
          uri="http://www.tibco.com/models/bomnotation">
    </package>
 </extension> 
 <extension
       point="com.tibco.xpd.resources.itemProviderAdapterOverride">
    <factory
          factory="com.tibco.xpd.bom.resources.ui.providers.BOMItemProviderAdapterFactory">
    </factory>
 </extension>
 <extension
       point="org.eclipse.ui.preferencePages">
    <page
          class="com.tibco.xpd.bom.resources.ui.prefs.BOMPreferencePage"
          id="com.tibco.xpd.bom.pref"
          name="%bomPrefPage_title">
    </page>
    <page
          category="com.tibco.xpd.bom.pref"
          class="com.tibco.xpd.bom.resources.ui.prefs.BOMPickerPreferencePage"
          id="com.tibco.xpd.bom.pref.picker"
          name="%pickerPreferencePage_title">
    </page>
 </extension>
 <extension
       point="com.tibco.xpd.validation.provider">
    <issue
          id="bom.duplicateResourceIssue"
          message="%bom_duplicateResource_longdesc"
          severity="ERROR">
    </issue>
 </extension>
 
    <extension
          point="com.tibco.xpd.resources.workingCopyIndexer">
       <indexer
             filePattern="bom"
             indexID="com.tibco.xpd.bom.resources.indexing.bomIndexer"
             indexerClass="com.tibco.xpd.bom.resources.ui.internal.indexing.BOMResourceIndexProvider">
          <additionalAttributes>
             <attribute
                   maxLength="2098"
                   name="CaseOrGlobalType">
             </attribute>
             <attribute
                   maxLength="4096"
                   name="image_uri">
             </attribute>
             <attribute
                   maxLength="4096"
                   name="cds_factory">
             </attribute>
             <attribute
                   maxLength="4096"
                   name="namespace_uri">
             </attribute>
             <attribute
                   maxLength="4096"
                   name="displayLabel">
             </attribute>
              <attribute
                   maxLength="4096"
                   name="packageLowerCase">
             </attribute>
          </additionalAttributes>
       </indexer>
    </extension>
    <extension
          point="com.tibco.xpd.resources.workingCopyIndexer">
       <indexer
             filePattern="uml"
             indexID="com.tibco.xpd.bom.resources.indexing.umlProfileIndexer"
             indexerClass="com.tibco.xpd.bom.resources.ui.internal.indexing.UMLProfileIndexProvider">
          <additionalAttributes>
             <attribute
                   maxLength="4096"
                   name="image_uri">
             </attribute>
            <attribute
                   maxLength="4096"
                   name="eclipse_project">
             </attribute>
          </additionalAttributes>
       </indexer>
       <indexer
             filePattern="uml"
             indexID="com.tibco.xpd.bom.resources.indexing.umlStereotypeIndexer"
             indexerClass="com.tibco.xpd.bom.resources.ui.internal.indexing.UMLStereotypeIndexProvider">
          <additionalAttributes>
            <attribute
                   maxLength="4096"
                   name="image_uri">
             </attribute>
            <attribute
                   maxLength="4096"
                   name="eclipse_project">
             </attribute>
            <attribute
                  maxLength="1"
                  name="model">
            </attribute>
            <attribute
                  maxLength="1"
                  name="package">
            </attribute>
            <attribute
                  maxLength="1"
                  name="class">
            </attribute>
            <attribute
                  maxLength="1"
                  name="property">
            </attribute>
            <attribute
                  maxLength="1"
                  name="primtype">
            </attribute>
            <attribute
                  maxLength="1"
                  name="generalization">
            </attribute>
            <attribute
                  maxLength="1"
                  name="association">
            </attribute>
            <attribute
                  maxLength="1"
                  name="operation">
            </attribute>
            <attribute
                  maxLength="1"
                  name="enumeration">
            </attribute>
            <attribute
                  maxLength="1"
                  name="enumLit">
            </attribute>
            <attribute
                  maxLength="1"
                  name="assocClass">
            </attribute>
          </additionalAttributes>
       </indexer>
    </extension>
 
  <extension
       point="org.eclipse.ui.exportWizards">
    <category
          id="com.tibco.xpd.bom.resources.ui.exportWizard.concept.category"
          name="%category.exportWizard.name"/>
