                 typeId="com.tibco.xpd.bom.resources.ui.types.Class">
           </type>
           <type
                 groupId="org.eclipse.emf.ecore.EObject"
                 typeId="com.tibco.xpd.bom.resources.ui.types.PrimitiveType">
           </type>
           <type
                 groupId="org.eclipse.emf.ecore.EObject"
                 typeId="com.tibco.xpd.bom.resources.ui.types.BasePrimitiveType">
           </type>
           <type
                 groupId="org.eclipse.emf.ecore.EObject"
                 typeId="com.tibco.xpd.bom.resources.ui.types.Stereotype">
           </type>
           <type
                 groupId="org.eclipse.emf.ecore.EObject"
                 typeId="com.tibco.xpd.bom.resources.ui.types.Profile">
           </type>
           <type
                 groupId="org.eclipse.emf.ecore.EObject"
                 typeId="com.tibco.xpd.bom.resources.ui.types.Enumeration">
           </type>
        </types>
        <filters>
           <filter
