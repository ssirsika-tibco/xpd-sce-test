  <extension
        point="org.eclipse.ltk.core.refactoring.renameParticipants">
     <renameParticipant
           class="com.tibco.xpd.bom.resources.ui.refactoring.BOMElementRefactorParticipant"
           id="com.tibco.xpd.bom.resources.ui.bomElementRenameParticipant"
           name="%renameBomElement">
        <enablement>
           <or>
