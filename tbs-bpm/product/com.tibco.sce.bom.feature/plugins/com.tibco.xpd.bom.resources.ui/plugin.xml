                 typeId="com.tibco.xpd.bom.resources.ui.types.Class">
           </filter>
           <filter
                 class="com.tibco.xpd.bom.resources.ui.types.internal.Filter"
                 typeId="com.tibco.xpd.bom.resources.ui.types.PrimitiveType">
