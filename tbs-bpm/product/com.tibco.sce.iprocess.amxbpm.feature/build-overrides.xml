<?xml version='1.0' encoding='UTF-8'?>
<project name="Business Studio Customer API Feature Overrides">

	<!-- Switches obfuscator style to none. 
    This needed to be switched off in version 3.8 to overcome problems with string externalization.
    -->
	<property name="autobuild.obfuscator.style" value="none" />
	
	<property name="autobuild.version.eclipse" value="4.7" />
	<property name="autobuild.version.eclipse-host" value="4.7" />
	
	<property name="release-unit" value="com.tibco.xpd.iprocess.amxbpm.feature" />
	<property name="feature" value="com.tibco.xpd.iprocess.amxbpm.feature" />
	
	<property name="autobuild.repository" value="xpd-repository" />
	
	<property name="javacSource" value="1.8" />
	<property name="javacTarget" value="1.8" />
	<property name="autobuild.jdk.force-18" value="true" />
	
	<property name="autobuild.p2.packaging" value="true" />

	<!-- allows for cycles in the binary dependencies -->
    <property name="allow.binary.cycles" value="true" />
	
	<updatesites id="autobuild.workspace.update-sites.list">
		<updatesite url="http://reldist.na.tibco.com/reldist/debug/site.xml" />
	</updatesites>
</project>
