<?xml version="1.0" encoding="UTF-8"?>
<templates>
<template id="com.tibco.xpd.script.javascript.templates.javascript.do" name="do" description="do-while" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">do {
	${line_selection}${cursor}
} while (${condition});</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.for" name="for" description="Simple loop" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">for(var ${index}=0; ${index}&lt;${array}.length; ${index}++) {
	${line_selection}${cursor}
}</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.forcollection" name="forcollection" description="loop through collections" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">for(var ${iterator}=${collection}.listIterator();${iterator}.hasNext();) {
	 var type = ${iterator}.next();
}</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.function" name="function" description="simple function with comment" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">/**
 * ${name}
 * @param {${type}} ${param} 
 */
 function ${name}(${param}) {
 	${cursor}
 }</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.ifelse" name="ifelse" description="if - else " context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">if (${condition}) {
	${cursor}
} else {
	
}
</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.ifelseif" name="ifelseif" description="if - elseif - else " context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">if (${condition}) {
	${cursor}
} else if (${condition}) {
	${cursor}
}else {
	
}
</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.switch" name="switch" description="switch" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">switch (${key}) {
	case ${value}${cursor}:
		break;
	default :
		break;
}
</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.try" name="try" description="try catch" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">try {
	${line_selection}${cursor}
} catch (e) {
	// TODO: handle exception
}</template>
<template id="com.tibco.xpd.script.javascript.templates.javascript.while" name="while" description="while loop" context="com.tibco.xpd.script.javascript.template.context" enabled="true" deleted="false" autoinsert="true">while (${condition}) {
	${line_selection}${cursor}
}
</template>
