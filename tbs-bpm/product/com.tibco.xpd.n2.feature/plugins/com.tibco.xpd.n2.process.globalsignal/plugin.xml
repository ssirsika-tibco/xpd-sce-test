                <instanceof
                     value="com.tibco.xpd.globalSignalDefinition.PayloadDataField">
                  </instanceof>
          </enablement>
     </renameParticipant>
     <renameParticipant
           class="com.tibco.xpd.n2.process.globalsignal.refactor.UpdateGsReferencesOnGsdFileRenameParticipant"
           id="com.tibco.xpd.n2.process.globalsignal.refactor.UpdateGsReferencesOnGsdFileRenameParticipant"
           name="%renameGsd_label">
	        <enablement>
	             <and>
	                <instanceof
	                      value="org.eclipse.core.resources.IFile">
