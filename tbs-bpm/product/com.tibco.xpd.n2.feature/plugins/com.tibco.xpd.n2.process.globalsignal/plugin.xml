             </or>
          </enablement>
     </renameParticipant>
     <renameParticipant
             class="com.tibco.xpd.n2.process.globalsignal.refactor.GlobalSignalReferenceParticipant"
             id="com.tibco.xpd.n2.globalsignal.resource.ui.GlobalSignalReferenceParticipant"
             name="%globalSignalRefUpdate">
