             class="com.tibco.xpd.n2.process.globalsignal.refactor.PayloadDataMappingReferenceParticipant"
             id="com.tibco.xpd.n2.globalsignal.resource.ui.PayloadDataMappingReferenceParticipant"
             name="%pdfRefUpdate">
