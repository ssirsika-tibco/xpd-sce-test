<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="systemActions" name="System Actions" schema="schema/systemActions.exsd"/>


   <extension
       point="com.tibco.xpd.resources.workingCopyFactory">
    <factory
          class="com.tibco.xpd.om.resources.wc.OMWorkingCopyFactory"
          filePattern=".*\.[oO][mM]">
    </factory>
 </extension>
   <extension
         point="com.tibco.xpd.resources.projectVersionRefactoringParticipant">
      <participant
            class="com.tibco.xpd.om.resources.internal.OMVersionRefactorParticipant"
            id="com.tibco.xpd.om.resource.projectVersionRefactorParticipant"
            name="%OMVersionRefactoring_participant_name">
         <enablement>
            <instanceof
                  value="org.eclipse.core.resources.IProject">
            </instanceof>
         </enablement>
      </participant>
   </extension>

</plugin>
