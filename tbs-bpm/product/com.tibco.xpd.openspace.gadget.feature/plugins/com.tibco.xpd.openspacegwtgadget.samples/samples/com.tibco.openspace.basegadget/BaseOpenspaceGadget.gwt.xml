<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- Inherit dependencies of the Gadget -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name='com.google.gwt.gadgets.Gadgets' />
	<inherits name="com.tibco.bpm.web.client.services.Services"/>
 	<inherits name="com.tibco.bpm.web.client.model.Model"/>
	<inherits name="com.google.gwt.json.JSON"/>
	<inherits name="com.google.gwt.xml.XML" />
	<inherits name="org.jsonmaker.gwt.Gwt_jsonmaker" />
  	<inherits name='com.tibco.forms.FormRunner'/>
	
  	<!-- Generate the Managed event generate to enable gadget to gadget communication -->
	<generate-with class="com.tibco.bpm.web.client.model.pagebus.rebind.ManagedEventGenerator">
	    <when-type-assignable class="com.tibco.bpm.web.client.model.pagebus.hub.ManagedEvent"/>
	</generate-with>
  	<!-- Import the base stylesheet -->
	<stylesheet src="gadget.css" />
	
	
	<source path="client"/>
</module>