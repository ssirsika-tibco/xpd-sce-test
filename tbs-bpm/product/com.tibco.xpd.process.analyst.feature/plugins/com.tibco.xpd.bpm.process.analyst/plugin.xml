   <extension
         name="BPMN Validation Provider for Ananylst Feature rules excluded from process editor feature"
         point="com.tibco.xpd.validation.provider">
      <provider
            id="com.tibco.xpd.bpm.process.analyst.bpmnValidationProvider"
            markerType="com.tibco.xpd.validation.bpmnValidationMarker"
            name="BPMN Validation Provider for Ananylst Feature rules excluded from process editor feature"
            scopeProvider="com.tibco.xpd.validation.xpdl2.provider.Xpdl2ScopeProvider">
         <rule
               class="com.tibco.xpd.bpm.om.validation.bpmn.rules.UnresolvedParticipantExternalReferenceRule">
         </rule>
      </provider>
      <issue
            id="bpmn.unresolvedParticipantExternalReference"
            message="%bpmn.unresolvedParticipantExternalReference"
            severity="ERROR">
         <resolution
               canApplyToMultiple="false"
               id="resolution.bpmn.selectExternalParticipant">
         </resolution>
      </issue>
      <issue
            id="bpmn.unsetParticipantExternalReference"
            message="%bpmn.unsetParticipantExternalReference"
            severity="ERROR">
