            issueId="processPackage.duplicateResourceIssue"
            kind="processes"
            name="%folder.name.processPackages"
            navigatorContentId="com.tibco.xpd.projectExplorer.bpmContent"
            projectAssetId="com.tibco.xpd.asset.businessProcess">
