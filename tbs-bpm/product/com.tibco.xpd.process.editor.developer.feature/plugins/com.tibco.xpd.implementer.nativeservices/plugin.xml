   </extension>
    <extension
         point="com.tibco.xpd.analyst.resources.xpdl2.bpmnCatchableErrorProviders">
      <ErrorBrowser
            class="com.tibco.xpd.implementer.nativeservices.globaldataservice.GlobalDataTaskErrorsProvider">
      </ErrorBrowser>
   </extension>
</plugin>
