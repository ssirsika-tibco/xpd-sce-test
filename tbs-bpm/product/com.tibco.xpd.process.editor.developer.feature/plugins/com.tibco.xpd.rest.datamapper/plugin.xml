<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="com.tibco.xpd.processeditor.xpdl2.propertyContributor">
         <propertySection
               class="com.tibco.xpd.rest.datamapper.RestDataMapperInSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.RestActivityInMappingFilter"
               id="com.tibco.xpd.rest.datamapper.in"
               tab="com.tibco.xpd.implementer.propertyTabs.parameterMapperIn">
         </propertySection>
         <propertySection
               class="com.tibco.xpd.rest.datamapper.RestDataMapperOutSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.RestActivityOutMappingFilter"
               id="com.tibco.xpd.rest.datamapper.out"
               tab="com.tibco.xpd.implementer.propertyTabs.parameterMapperOut">
         </propertySection>
         <propertySection
               class="com.tibco.xpd.rest.datamapper.RestDataMapperOutSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.RestCatchInMappingFilter"
               id="com.tibco.xpd.rest.datamapper.catch"
               tab="com.tibco.xpd.processeditor.propertyTabs.CatchErrorEventMapperOut">
         </propertySection>
         <propertySection
               class="com.tibco.xpd.swagger.datamapper.SwaggerDataMapperInSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.SwaggerActivityInMappingFilter"
               id="com.tibco.xpd.swagger.datamapper.in"
               tab="com.tibco.xpd.implementer.propertyTabs.parameterMapperIn">
         </propertySection>
         <propertySection
               class="com.tibco.xpd.swagger.datamapper.SwaggerDataMapperOutSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.SwaggerActivityOutMappingFilter"
               id="com.tibco.xpd.swagger.datamapper.out"
               tab="com.tibco.xpd.implementer.propertyTabs.parameterMapperOut">
         </propertySection>
         <propertySection
               class="com.tibco.xpd.swagger.datamapper.SwaggerDataMapperOutSection"
               enablesFor="1"
               filter="com.tibco.xpd.implementer.resources.xpdl2.properties.SwaggerCatchInMappingFilter"
               id="com.tibco.xpd.swagger.datamapper.catch"
               tab="com.tibco.xpd.processeditor.propertyTabs.CatchErrorEventMapperOut">
         </propertySection>
      </propertySections>
   </extension>

   <extension
         point="com.tibco.xpd.script.ui.scriptContexts">
      <scriptContext
            type="DataMapperRestMappingScripts">
      </scriptContext>
   </extension>
   <extension
         point="com.tibco.xpd.analyst.resources.xpdl2.processPreCommitCommandContributor">
      <ProcessPreCommitCommandContributor
            class="com.tibco.xpd.rest.datamapper.precommit.RestInvocationPreCommitListener"
            run="After">
      </ProcessPreCommitCommandContributor>
   </extension>
	<extension
          point="com.tibco.xpd.processeditor.xpdl2.dropObjectContribution">
          <dropObject
                class="com.tibco.xpd.rest.dragdrop.RestMethodDropContribution"
                name="REST Method Drop">
          </dropObject>
     </extension>
    <extension
         point="com.tibco.xpd.datamapper.DataMapperContentContribution">
               <mapperContentContributor
            applicableContext="ProcessToRestService"
            applyToMapperRHS="false"
            mapperContribution="com.tibco.xpd.process.datamapper.common.ActivityInterfaceDataMapperContentContributor"
            priority="2">
      </mapperContentContributor>
      <mapperContentContributor
            applicableContext="RestServiceToProcess"
            applyToMapperRHS="true"
            mapperContribution="com.tibco.xpd.process.datamapper.common.ActivityInterfaceDataMapperContentContributor"
            priority="2">
      </mapperContentContributor>
      <mapperContentContributor
            applicableContext="ProcessToRestService"
            applyToMapperRHS="true"
            mapperContribution="com.tibco.xpd.rest.datamapper.RestDataMapperInContentContributor"
            priority="3">
      </mapperContentContributor>
            <mapperContentContributor
            applicableContext="RestServiceToProcess"
            applyToMapperRHS="false"
            mapperContribution="com.tibco.xpd.rest.datamapper.RestDataMapperOutContentContributor"
            priority="3">
      </mapperContentContributor>      
      <mapperContentContributor
            applicableContext="ProcessToSwagger"
            applyToMapperRHS="false"
            mapperContribution="com.tibco.xpd.process.datamapper.common.ActivityInterfaceDataMapperContentContributor"
            priority="2">
      </mapperContentContributor>
      <mapperContentContributor
            applicableContext="SwaggerToProcess"
            applyToMapperRHS="true"
            mapperContribution="com.tibco.xpd.process.datamapper.common.ActivityInterfaceDataMapperContentContributor"
            priority="2">
      </mapperContentContributor>
      <mapperContentContributor
            applicableContext="ProcessToSwagger"
            applyToMapperRHS="true"
            mapperContribution="com.tibco.xpd.swagger.datamapper.SwaggerDataMapperInContentContributor"
            priority="3">
      </mapperContentContributor>
            <mapperContentContributor
            applicableContext="SwaggerToProcess"
            applyToMapperRHS="false"
            mapperContribution="com.tibco.xpd.swagger.datamapper.SwaggerDataMapperOutContentContributor"
            priority="3">
      </mapperContentContributor>        
     </extension>
    <extension
          point="com.tibco.xpd.process.datamapper.dataReferenceContextProvider">
       <provider
             providerClass="com.tibco.xpd.rest.datamapper.RestDataReferenceContextProvider">
       </provider>
    </extension>
    
    <extension
         point="com.tibco.xpd.processeditor.xpdl2.advancedProperties">
      <DestinationEnvironments
            DestinationEnvCategoryLabel="%property_category_rest_service_input">
         <DestinationEnvironment
               DestinationEnvironmentId="bpmn1">
         </DestinationEnvironment>
      </DestinationEnvironments>
      <AdvancedProperties>
         <AdvancedProperty
               ContributedAdvancedPropertyClass="com.tibco.xpd.rest.datamapper.properties.RestInputExcludeEmptyOptionalObjectsProperty"
               DisplayName="%property_exclude_empty_optional_objects_from_target_data">
         </AdvancedProperty>
         <AdvancedProperty
               ContributedAdvancedPropertyClass="com.tibco.xpd.rest.datamapper.properties.RestInputExcludeEmptyObjectsFromArraysProperty"
               DisplayName="%property_exclude_empty_objects_from_arrays_in_target_data">
         </AdvancedProperty>
         <AdvancedProperty
               ContributedAdvancedPropertyClass="com.tibco.xpd.rest.datamapper.properties.RestInputExcludeEmptyOptionalArraysProperty"
               DisplayName="%property_exclude_empty_optional_arrays_from_target_data">
         </AdvancedProperty>
        </AdvancedProperties>
   </extension>
</plugin>
