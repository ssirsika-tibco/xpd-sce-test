<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/XSDBOM_AttributeGroup01A_SimpleAttrGroup"
	xmlns:tns="http://www.example.org/XSDBOM_AttributeGroup01A_SimpleAttrGroup"
	elementFormDefault="qualified">

	<attributeGroup name="detailsGroup">
		<attribute name="firstname" type="string" />
		<attribute name="surname" type="string" />
		<attribute name="email" type="string" />
	</attributeGroup>

	<complexType name="PersonDetails">
		<attributeGroup ref="tns:detailsGroup" />
	</complexType>


</schema>