        <xpdl2:Activity Id="_-m6gIhOaEe-fuvwZ8PuNhw" Name="invalidPslFunctionCallFromPslFunctionTests" xpdExt:UseIn="All">
          <xpdl2:Description>Add your function summary here...</xpdl2:Description>
          <xpdl2:Implementation>
            <xpdl2:Task>
              <xpdl2:TaskScript>
                <xpdl2:Script ScriptGrammar="JavaScript">//&#xD;
// Tests for invalid PSL function call from PSL function use cases.&#xD;
//&#xD;
     &#xD;
// Incorrect BOM Case class parameter type&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomCaseParamAndReturn(srcCase2Bom, srcCase1Bom); &#xD;
// Incorrect BOM Case Class parameter type - Same NAME but from different BOM package&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomCaseParamAndReturn(anotherPkgCase1Bom, srcCase2Bom);  &#xD;
// Incorrect BOM Case Class parameter type - factory method for wrong class type&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomCaseParamAndReturn(factory.com_example_data.createCase2(), factory.com_example_data.createCase1());  &#xD;
// Incorrect BOM Case Class parameter type - factory method for SAME NAME class but from different BOM package&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomCaseParamAndReturn(factory.com_example_another.createCase1(), factory.com_example_another.createCase2());  &#xD;
// Incorrect BOM Case class return type assignment&#xD;
srcCase2Bom = bpmScripts.Utilities.UtilitiesLibrary.bomCaseParamAndReturn(srcCase1Bom, srcCase2Bom);  &#xD;
&#xD;
// Incorrect BOM standard class parameter type&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(srcClass2Bom, srcClass1Bom);&#xD;
// Same but where source param is BOM child attribute&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(srcClass1Bom, srcClass2Bom.class1);&#xD;
// Incorrect BOM standard Class parameter type - Same NAME but from different BOM package&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(anotherPkgClass1Bom, srcCase2Bom);  &#xD;
// Incorrect BOM standard Class parameter type - factory method for wrong class type&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(factory.com_example_data.createClass2(), factory.com_example_data.createClass1());  &#xD;
// Incorrect BOM standard Class parameter type - factory method for SAME NAME class but from different BOM package&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(factory.com_example_another.createClass1(), factory.com_example_another.createClass2());  &#xD;
&#xD;
// Incorrect BOM standard class return type assignment&#xD;
srcClass2Bom = bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(srcClass1Bom, srcClass2Bom);&#xD;
// BOM standard class params where src is incorrect class return from other PSL function&#xD;
bpmScripts.Utilities.UtilitiesLibrary.bomClassParamAndReturn(srcClass1Bom, bpmScripts.Utilities.UtilitiesLibrary.returnClass1());&#xD;
&#xD;
// Incorrect Case-type Case References passed for specific-case-typed params&#xD;
bpmScripts.Utilities.UtilitiesLibrary.caseRefParamAndReturn(srcCase2Ref, srcCase1Ref);&#xD;
// Incorrect assignment to wrong case-ref type from specific-case-typed case-ref return&#xD;
srcCase2Ref = bpmScripts.Utilities.UtilitiesLibrary.caseRefParamAndReturn(srcCase1Ref, srcCase2Ref);&#xD;
// Incorrect assignment Case Object from specific-case-typed  case-ref return of same type&#xD;
srcCase1Bom = bpmScripts.Utilities.UtilitiesLibrary.caseRefParamAndReturn(srcCase1Ref, srcCase2Ref);&#xD;
// Incorrect Case References where src param is return from other PSL function&#xD;
bpmScripts.Utilities.UtilitiesLibrary.caseRefParamAndReturn(srcCase1Ref, bpmScripts.Utilities.UtilitiesLibrary.returnCase1Ref());&#xD;
&#xD;
// Invalid Primitive types parameter passing&#xD;
bpmScripts.Utilities.UtilitiesLibrary.primitiveTypes(srcText, srcText, srcDateTime);&#xD;
// Incorrect  return type assignment&#xD;
srcNumber = bpmScripts.Utilities.UtilitiesLibrary.primitiveTypes(srcText, srcNumber, srcDateTime);&#xD;
&#xD;
// Array BOM Case classes pass wrong type array as parameter&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayBomCaseParamAndReturn(srcArrayCase2Bom, srcArrayCase1Bom); &#xD;
// Array BOM Case classes pass correct type bit NON-array as parameter&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayBomCaseParamAndReturn(srcCase1Bom, srcCase2Bom); &#xD;
// Array BOM Case classes assignment from wrong return type&#xD;
srcArrayCase2Bom = bpmScripts.Utilities.UtilitiesLibrary.arrayBomCaseParamAndReturn(srcArrayCase1Bom, srcArrayCase2Bom); &#xD;
// Array BOM Case classes assignment to correct but NON-array return type&#xD;
srcCase1Bom = bpmScripts.Utilities.UtilitiesLibrary.arrayBomCaseParamAndReturn(srcArrayCase1Bom, srcArrayCase2Bom); &#xD;
&#xD;
// Array BOM standard classes pass wrong type array as parameter&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayBomClassParamAndReturn(srcArrayClass2Bom, srcArrayClass1Bom); &#xD;
// Array BOM standard classes pass correct type bit NON-array as parameter&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayBomClassParamAndReturn(srcClass1Bom, srcClass2Bom); &#xD;
// Array BOM standard classes assignment from wrong return type&#xD;
srcArrayClass2Bom = bpmScripts.Utilities.UtilitiesLibrary.arrayBomClassParamAndReturn(srcArrayClass1Bom, srcArrayClass2Bom); &#xD;
// Array BOM standard classes assignment to correct but NON-array return type&#xD;
srcClass1Bom = bpmScripts.Utilities.UtilitiesLibrary.arrayBomClassParamAndReturn(srcArrayClass1Bom, srcArrayClass2Bom); &#xD;
// Non-array BOM classes assignment to correct but array return type&#xD;
srcArrayClass1Bom = bpmScripts.Utilities.UtilitiesLibrary.returnClass1();&#xD;
&#xD;
// Incorrect array Case-type Case References passed for specific-case-typed params&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayCaseRefParamAndReturn(srcArrayCase2Ref, srcArrayCase1Ref);&#xD;
// Incorrect assignment from for Array specific-case-typed return&#xD;
srcArrayCase2Ref = bpmScripts.Utilities.UtilitiesLibrary.arrayCaseRefParamAndReturn(srcArrayCase1Ref, srcArrayCase2Ref);&#xD;
// Incorrect assignment Case Object from specific-case-typed  case-ref return of same type&#xD;
srcArrayCase2Bom = bpmScripts.Utilities.UtilitiesLibrary.arrayCaseRefParamAndReturn(srcArrayCase1Ref, srcArrayCase2Ref);&#xD;
// Incorrect Array Case References where src param is return from other PSL function&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayCaseRefParamAndReturn(srcArrayCase1Ref, bpmScripts.Utilities.UtilitiesLibrary.returnCase1Ref());&#xD;
&#xD;
// Array Primitive types incorrect parameter types&#xD;
bpmScripts.Utilities.UtilitiesLibrary.arrayPrimitiveTypes(srcArrayText, srcArrayText, srcArrayDateTime);&#xD;
// Array Primitive types incorrect return assignment  types&#xD;
srcArrayNumber = bpmScripts.Utilities.UtilitiesLibrary.arrayPrimitiveTypes(srcArrayText, srcArrayNumber, srcArrayDateTime);&#xD;
// Array Primitive types incorrect return assignment to correct but NON-array type&#xD;
srcDateTime = bpmScripts.Utilities.UtilitiesLibrary.arrayPrimitiveTypes(srcArrayText, srcArrayNumber, srcArrayDateTime);&#xD;
// Non-Array Primitive types incorrect return assignment to correct but array type&#xD;
srcArrayNumber = bpmScripts.Utilities.UtilitiesLibrary.returnNumber();&#xD;
&#xD;
</xpdl2:Script>
              </xpdl2:TaskScript>
            </xpdl2:Task>
