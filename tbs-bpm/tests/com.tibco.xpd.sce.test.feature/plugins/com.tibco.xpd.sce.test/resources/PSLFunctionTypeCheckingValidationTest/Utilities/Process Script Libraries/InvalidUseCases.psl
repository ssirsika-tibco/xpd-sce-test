        <xpdl2:Activity Id="_-m6gNhOaEe-fuvwZ8PuNhw" Name="invalidPslFunctionGeneralRegressionTests" xpdExt:UseIn="All">
          <xpdl2:Description>Add your function summary here...</xpdl2:Description>
          <xpdl2:Implementation>
            <xpdl2:Task>
              <xpdl2:TaskScript>
                <xpdl2:Script ScriptGrammar="JavaScript">//&#xD;
// Tests invalid use cases for general regression and normal pre-PSL scripting tests from PSL function.&#xD;
//&#xD;
&#xD;
// Basic assignments&#xD;
secondDateTime = srcText;&#xD;
secondCase1Ref = srcCase2Ref;&#xD;
secondCase1RefArray = srcArrayCase2Ref;&#xD;
secondCase1RefArray.pushAll(srcCase1Ref); &#xD;
secondCase1RefArray.pushAll(srcArrayCase2Ref); &#xD;
srcClass2Bom = srcClass2Bom.class1;&#xD;
 &#xD;
// Invocation of existing caseData methods that take generic 'any case / case-ref' as parameters / returns&#xD;
srcArrayCase1Bom = bpm.caseData.findAll(&quot;com.example.data.Case1AAA&quot;, 1, 10);&#xD;
srcCase1Bom = bpm.caseData.findByCaseIdentifier(srcText, &quot;com.example.data.Case1BBB&quot;);&#xD;
srcArrayCase1Bom = bpm.caseData.findByCriteria(srcText,&quot;com.example.data.Case1CCC&quot;,srcNumber, 10);&#xD;
srcArrayCase1Bom = bpm.caseData.findBySimpleSearch(srcText,&quot;com.example.data.Case1DDD&quot;,srcNumber, 10);&#xD;
srcCase1Ref = bpm.caseData.read(srcCase1Ref);&#xD;
srcArrayCase1Ref = bpm.caseData.readAll(srcArrayCase1Ref);&#xD;
srcClass1Bom = bpm.caseData.readAll(srcArrayCase1Ref);&#xD;
bpm.caseData.readAll(srcArrayCase1Bom);&#xD;
&#xD;
// Invocation of utilitiy methods that accept / return generic 'any BOM' values&#xD;
srcCase1Bom = bpm.scriptUtil.copy(srcArrayText);&#xD;
srcText = bpm.scriptUtil.copy(srcArrayCase1Ref);&#xD;
secondCase1Ref = bpm.scriptUtil.copy(srcCase1Ref); // We should be careful about what we allow to be assigned to case-refs!&#xD;
secondCase1BomArray = bpm.scriptUtil.copyAll(srcCase1Bom);&#xD;
secondCase1Bom = bpm.scriptUtil.copyAll(srcArrayCase1Bom);&#xD;
srcArrayText = bpm.scriptUtil.copyAll(srcCase1Ref); &#xD;
secondCase1RefArray = bpm.scriptUtil.copyAll(srcArrayCase1Ref); // We should be careful about what we allow to be assigned to case-refs!&#xD;
secondCase1Ref = bpm.scriptUtil.parseData(srcText, &quot;com.example.data.Case1XXX&quot;);&#xD;
secondCase1Ref = bpm.scriptUtil.stringify(srcCase1Bom);&#xD;
&#xD;
// Use BOM parameter attributes&#xD;
srcClass2Bom = srcClass2Bom.class1;&#xD;
srcText = srcClass2Bom.class1.charAt(srcNumber);&#xD;
&#xD;
// Use PSL function returns in params&#xD;
// Primitive type return&#xD;
bpm.caseData.findAll(&quot;com.example.data.Case1YYY&quot;, bpmScripts.Utilities.UtilitiesLibrary.returnCase1Ref(), bpmScripts.Utilities.UtilitiesLibrary.returnClass1());&#xD;
// Case reference function return used as parameter and case object return&#xD;
srcCase1Bom = bpm.caseData.read(bpmScripts.Utilities.UtilitiesLibrary.returnText());&#xD;
srcCase1Ref = bpm.caseData.read(bpmScripts.Utilities.UtilitiesLibrary.returnCase1Ref());&#xD;
&#xD;
// Assignment of Case / BOM classes from factory methods&#xD;
srcCase1Ref = factory.com_example_data.createCase1();&#xD;
srcCase2Bom = factory.com_example_data.createCase1();&#xD;
srcClass2Bom = factory.com_example_data.createClass1();&#xD;
&#xD;
</xpdl2:Script>
              </xpdl2:TaskScript>
