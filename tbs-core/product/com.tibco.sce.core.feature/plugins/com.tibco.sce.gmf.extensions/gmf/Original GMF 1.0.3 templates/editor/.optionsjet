<%@ jet package="org.eclipse.gmf.codegen.templates.editor" class="OptionsFileGenerator"
    imports="org.eclipse.gmf.codegen.gmfgen.*"%>
<%GenPlugin genPlugin = (GenPlugin) argument;%>
# Debugging options for the <%=genPlugin.getID()%> plug-in

# Turn on general debugging for the <%=genPlugin.getID()%> plug-in
<%=genPlugin.getID()%>/debug=false

# Turn on debugging of visualID processing
<%=genPlugin.getID()%>/debug/visualID=true