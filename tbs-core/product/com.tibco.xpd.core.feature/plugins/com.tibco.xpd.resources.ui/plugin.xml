            class="com.tibco.xpd.resources.ui.internal.WorkingCopyResourceMoveRefactorParticipant"
            id="com.tibco.xpd.resources.ui.workingCopyRsourceMoveRefactorParticipant"
            name="%workingCopyRsourceMoveRefactorParticipantName">
         <enablement>
            <instanceof
                  value="org.eclipse.core.resources.IFile">
            </instanceof></enablement>
      </moveParticipant>
   </extension>

         <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.tibco.xpd.ui.projectexplorer.providers.CommonSpecialFolderContentProvider"
            icon="icons/Project16np.gif"
            id="com.tibco.xpd.projectExplorer.specialFolderContent"
            labelProvider="com.tibco.xpd.ui.projectexplorer.providers.ProjectExplorerLabelProvider"
            name="Special Folder Content"
            priority="highest">
       <override
             policy="InvokeAlwaysRegardlessOfSuppressedExt"
             suppressedExtensionId="org.eclipse.ui.navigator.resourceContent">
       </override>
         <triggerPoints>
         <or>
            <and>
					<instanceof value="org.eclipse.core.resources.IProject"/>
						<test
							property="org.eclipse.core.resources.projectNature"
	                        value="com.tibco.xpd.resources.xpdNature"/>
	                    <test
	                        property="org.eclipse.core.resources.open"
	                        value="true"/>
				</and>
				<and>
					<instanceof value="org.eclipse.core.resources.IFolder"/>					 
					<test
						property="com.tibco.xpd.resources.specialfolder.containsSpecialFolder"/>
     				<not>	
     					<test property="org.eclipse.core.resources.name" value=".*"/>
     				</not>
   
				</and>						
				<instanceof value="org.eclipse.core.resources.IWorkspaceRoot"/>
			</or>
         </triggerPoints>
         
         <commonSorter
               class="com.tibco.xpd.ui.projectexplorer.sorter.XpdProjectContentSorter"
               id="com.tibco.xpd.projectExplorer.xpdProjectContentSorter.sorter">
            <parentExpression>
               <or>
                  <instanceof
                         value="org.eclipse.core.resources.IProject">
                   </instanceof>
                   <instanceof
                        value="org.eclipse.core.resources.IWorkspaceRoot">
                  </instanceof>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            class="com.tibco.xpd.ui.projectexplorer.viewerfilters.SpecialFolderIFolderRemovalFilter"
            id="com.tibco.xpd.projectExplorer.specialFolderFilter"
            name="%special_folder_filter.label"
            visibleInUI="true">
      </commonFilter>
       </extension>
         <extension
               point="org.eclipse.ui.handlers">
            <handler
                  class="com.tibco.xpd.resources.ui.internal.actions.PropertiesSectionSaveHandler"
                  commandId="org.eclipse.ui.file.save">
               <activeWhen>
               	<and>
               		<with variable="activePartId">
    					<equals value="org.eclipse.ui.views.PropertySheet"/>
   					</with>
   					<not>
                       	<test
                             property="org.eclipse.core.resources.projectNature"
                             value="com.tibco.bw.design.bwNature">
                       </test>
                   </not>
   				</and>
               </activeWhen>
            </handler>
         </extension>
         <extension
               point="org.eclipse.ui.menus">
            <menuContribution
                  allPopups="false"
                  locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
               <menu
                     id="xpd.refactor"
                     label="%refactor_menu">
                  <separator
                        name="main">
                  </separator>
                  <separator
                        name="additions">
                  </separator>
                  <visibleWhen
                        checkEnabled="false">
                     <iterate>
                        <and>
                           <instanceof
                                 value="org.eclipse.core.resources.IProject">
                           </instanceof>       
                           <test
                                 property="com.tibco.xpd.projectNatureTester.hasNature"
                                 value="com.tibco.xpd.resources.xpdNature">
                           </test>                    
                        </and>
                     </iterate>
                  </visibleWhen>
               </menu>
            </menuContribution>
         </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.tibco.xpd.ui.perspective.PerspactiveLifcycleStartup"></startup>
   </extension>    
</plugin>
