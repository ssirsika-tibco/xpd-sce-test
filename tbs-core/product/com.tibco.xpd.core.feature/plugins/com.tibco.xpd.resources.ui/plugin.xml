   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  pattern="com.tibco.xpd.projectExplorer.*"/>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <actionExtension pattern="com.tibco.xpd.projectExplorer.action.*"/>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.SpecialFoldersActionProvider"
            id="com.tibco.xpd.projectExplorer.action.specialFolders">
         <enablement>
            <or>
               <instanceof value="org.eclipse.core.resources.IFolder"/>
               <instanceof value="com.tibco.xpd.resources.projectconfig.SpecialFolder"/>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.SpecialFolderRefactorActionProvider"
            id="com.tibco.xpd.projectExplorer.action.specialFoldersRefactor">
         <enablement>
            <instanceof value="com.tibco.xpd.resources.projectconfig.SpecialFolder"/>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.SpecialFolderEditActionProvider"
            id="com.tibco.xpd.projectExplorer.action.specialFoldersEdit">
         <enablement>
            <instanceof value="com.tibco.xpd.resources.projectconfig.SpecialFolder"/>
         </enablement>
      </actionProvider>      
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.WorkingCopyRevertActionProvider"
            id="com.tibco.xpd.projectExplorer.action.workingCopyRevertAction">
         <enablement>
            <instanceof value="org.eclipse.core.resources.IFile"/>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.ShowPropertiesViewActionProvider"
            id="com.tibco.xpd.projectExplorer.action.showPropertiesViewAction">
         <enablement>
         </enablement>
      </actionProvider>
       <!-- This is to enable back OpenActionProvider from navigator.resources as it is overwriten by java and js action 
          providers and is filtered when activities (java, js) are disabled (Non-BPM Features category). -->
      <actionProvider
            class="org.eclipse.ui.internal.navigator.resources.actions.OpenActionProvider"
            id="com.tibco.xpd.projectExplorer.action.OpenActions"
            overrides="org.eclipse.ui.navigator.resources.OpenActions"
            priority="high">
         <enablement>
            <or>
               <adapt
                     type="org.eclipse.core.resources.IFile">
               </adapt>
            </or>
         </enablement>
      </actionProvider>
