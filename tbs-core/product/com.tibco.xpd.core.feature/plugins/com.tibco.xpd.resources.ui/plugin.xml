                        value="org.eclipse.core.resources.IWorkspaceRoot">
                  </instanceof>
                  <and>
                     <instanceof
                           value="java.util.Collection">
                     </instanceof>
                     <count
                           value="0">
                     </count>
                  </and>
               </or>
          </enablement>
      </commonWizard>
      <commonWizard
