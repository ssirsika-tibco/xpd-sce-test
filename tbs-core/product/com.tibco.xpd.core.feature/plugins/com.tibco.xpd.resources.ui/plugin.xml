            id="com.tibco.xpd.projectExplorer.action.RefactorActions"
            overrides="org.eclipse.ui.navigator.resources.actions.RefactorActions">
         <enablement>
            <instanceof
                  value="org.eclipse.core.resources.IResource">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.xpd.resources.ui.internal.actions.FileRenameActionProvider"
            id="com.tibco.xpd.projectExplorer.action.FileRenameAction">
         <enablement>
            <and>
               <instanceof
                     value="org.eclipse.core.resources.IFile">
               </instanceof>
               <test
                     property="com.tibco.xpd.resources.wc.isWorkingCopyDirty">
               </test>
            </and>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.xpd.ui.projectexplorer.actions.providers.QuickSearchActionProvider"
            id="com.tibco.xpd.projectExplorer.action.find">
         <enablement></enablement>
      </actionProvider>
      <commonWizard
            menuGroupId="additions"
            type="export"
            wizardId="com.tibco.xpd.resources.ui.exportStudioProjectWizard">
         <enablement>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
               <instanceof
                     value="com.tibco.xpd.resources.projectconfig.SpecialFolder">
               </instanceof>
               <and>
                  <instanceof
                        value="java.util.Collection">
                  </instanceof>
                  <count
                        value="0">
                  </count>
               </and>
            </or>
         </enablement>
      </commonWizard>
      <!-- ACE-2011: Remove Unsupported features. Documentation
      <commonWizard
            menuGroupId="additions.xpd"
            type="export"
            wizardId="com.tibco.xpd.ui.importexport.exportwizard.documentation.DocExportWizard">
         <enablement>
            <and>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.eclipse.core.resources.projectNature"
                     value="com.tibco.xpd.resources.xpdNature">
               </test>
               <test
                     forcePluginActivation="true"
                     property="com.tibco.xpd.capabilityTester.isSolutionDesignEnabled"
                     value="true">
               </test>
            </and>
         </enablement>
      </commonWizard>
      -->
      <commonWizard
            menuGroupId="additions.xpd"
            type="import"
            wizardId="com.tibco.xpd.resources.ui.maaImport">
         <enablement>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IWorkspaceRoot">
               </instanceof>
               <and>
                  <instanceof
                        value="java.util.Collection">
                  </instanceof>
                  <count
                        value="0">
                  </count>
               </and>
            </or></enablement>
      </commonWizard>
      <commonWizard
            menuGroupId="additions.xpd"
            type="export"
            wizardId="com.tibco.xpd.resources.ui.maaExport">
         <enablement>
            <and>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.eclipse.core.resources.projectNature"
                     value="com.tibco.xpd.resources.xpdNature">
               </test>
            </and>
         </enablement>
      </commonWizard>
   </extension>
